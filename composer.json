{
  "name": "mbeym/tst-manual-scoring-question",
  "description": "An ILIAS plugin that modifies the manual scoring page",
  "type": "project",
  "license": "GPL",
  "authors": [
    {
      "name": "Marvin Beym",
      "email": "mBeym@databay.de"
    }
  ],
  "require": {
    "php": ">=7.4 <8.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "captainhook/plugin-composer": "^5.3",
    "friendsofphp/php-cs-fixer": "^3.16"
  },
  "autoload": {
    "psr-4": {
      "ILIAS\\Plugin\\TstManualScoringQuestion\\": "src/"
    },
    "classmap": [
      "./src"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "classmap-authoritative": true,
    "allow-plugins": {
      "captainhook/plugin-composer": true
    }
  }
}
